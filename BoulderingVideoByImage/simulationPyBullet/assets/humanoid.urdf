<?xml version="1.0"?>
<robot name="climbing_humanoid">

  <!-- MATERIAL DEFINITIONS -->
  <material name="blue">
    <color rgba="0.47 0.55 0.69 1"/>
  </material>
  <material name="light_blue">
    <color rgba="0.64 0.69 0.74 1"/>
  </material>
  <material name="dark_blue">
    <color rgba="0.35 0.41 0.47 1"/>
  </material>

  <!-- BASE LINK -->
  <link name="root">
    <inertial>
      <origin xyz="0 0.07 0" rpy="0 0 0"/>
      <mass value="6.0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    <visual>
      <geometry>
        <sphere radius="0.18"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.18"/>
      </geometry>
    </collision>
  </link>

  <!-- CHEST (SPHERICAL) -->
  <link name="chest">
    <inertial>
      <origin xyz="0 0.12 0" rpy="0 0 0"/>
      <mass value="14.0"/>
      <inertia ixx="0.2" ixy="0" ixz="0" iyy="0.2" iyz="0" izz="0.2"/>
    </inertial>
    <visual>
      <geometry>
        <sphere radius="0.22"/>
      </geometry>
      <material name="blue"/>
    </visual>
  </link>

  <joint name="chest_joint" type="spherical">
    <parent link="root"/>
    <child link="chest"/>
    <origin xyz="0 0.236 0" rpy="0 0 0"/>
    <limit lower="-1.2" upper="1.2" effort="200"/>
  </joint>

